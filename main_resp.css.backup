	font-weight: normal;
	line-height: 19px;
	color: #404040;
	}

.plashka .sub
{
	color: #404040;
}

.plashka .text, .plashka a .text {line-height: 16px; color: #404040;}

.plashkas .plashka .text, .plashka3s .plashka a .text { width: 313px; }
.plashka4s .plashka .text, .plashka4s .plashka a .text { width: 240px; }
.plashka5s .plashka .text, .plashka5s .plashka a .text { width: 180px; }
.plashka6s .plashka .text, .plashka5s .plashka a .text { width: 146px; }

.plashkas .plashka .withpic .text, .plashka3s .plashka a .withpic .text { width: 243px; }
.plashka4s .plashka .withpic .text, .plashka4s .plashka a .withpic .text { width: 160px; }
.plashka5s .plashka .withpic .text, .plashka5s .plashka a .withpic .text { width: 110px; }
.plashka6s .plashka .withpic .text, .plashka5s .plashka a .withpic .text { width: 76px; }

.plashka .author {
	font-weight: bold;
	color: #404040;
	display: block;
	margin-top:5px;
	margin-bottom: 1px;
	}

.plashka .pic img {
	width: 70px;
	height: auto;
	position: absolute;
	right: 0;
	bottom: 0;
	margin: 0;
	padding: 0;
}

